#!/usr/bin/env bash

cmd_exists() {
    unalias "$1" >/dev/null 2>&1 ||
    command -v "$1" >/dev/null 2>&1 ||
    exit 1
}

# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# execute function

cmd_exists "$@"

# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
